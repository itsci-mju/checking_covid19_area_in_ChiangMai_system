<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_vilage"
    android:padding="10dp"
    app:cardCornerRadius="8dp"
    app:cardElevation="8dp"
    tools:context=".admin.ListRiskAreaActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="15dp"
        android:background="#00FFFFFF"
        android:gravity="center"
        android:orientation="vertical">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="527dp">

            <LinearLayout
                android:id="@+id/showlistcluster_admin"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#00FFFFFF"
                android:orientation="vertical" />
        </ScrollView>

        <ImageView
            android:id="@+id/imagebackadmin"
            android:layout_width="319dp"
            android:layout_height="54dp"
            android:onClick="ClickBTNBackByAdmin"
            app:srcCompat="@drawable/co_back" />

    </LinearLayout>

</RelativeLayout>